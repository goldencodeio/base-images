[mysqld]
skip-host-cache
skip-name-resolve
connect_timeout=100
innodb_flush_log_at_trx_commit=2
innodb_flush_method=O_DIRECT
transaction-isolation=READ-COMMITTED
tmp_table_size=128M
key_buffer_size=128M
join_buffer_size=250M
sort_buffer_size=250M


# зависит от оперативы
innodb_buffer_pool_size=800M
#innodb_buffer_pool_size=256M
#optimizer_switch='index_merge_union=on,index_merge_sort_union=on,optimize_join_buffer_size=on,join_cache_bka=on,join_cache_hashed=on,join_cache_incremental=off'
